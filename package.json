    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.6.2",
    "franc-min": "^6.2.0",
    "js-beautify": "^1.15.1",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.2.7",
    "vue-tsc": "^2.0.6",
    "wxt": "^0.17.0"
  }
}
